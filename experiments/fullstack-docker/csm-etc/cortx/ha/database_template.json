{
    "databases": {
        "consul_db": {
            "import_path": "ConsulDB",
            "config": {
                "host": "<DOCKER_HOST_IP>",
                "port": 8500,
                "login": "",
                "password": ""
            }
        },
        "es_db": {
            "import_path": "ElasticSearchDB",
            "config": {
                "host": "<DOCKER_HOST_IP>",
                "port": 9200,
                "login": "",
                "password": "",
                "replication": 1
            }
        }

        {
            "import_path": "cortx.utils.ha.dm.models.decisiondb.DecisionModel",
            "database": "consul_db",
            "config": {
                "consul_db": {
                    "collection": "HA"
                }
            }
        },
        {
            "import_path": "cortx.utils.product_features.model.UnsupportedFeaturesModel",
            "database": "es_db",
            "config": {
                "es_db": {
                    "collection": "config"
                }
            }
        }

    ]

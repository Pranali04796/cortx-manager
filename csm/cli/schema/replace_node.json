{
  "name": "replace_node",
  "description": "Maintenance for System Nodes Command.",
  "sub_commands": [
    {
      "name": "start",
      "description": "Replace Not-Working Node.",
      "need_confirmation": true,
      "permissions_tag": "update",
      "args": [
        {
          "flag": "resource_name",
          "help": "Node Id for node to be replaced by new node.",
          "type": "str",
          "json": true
        },
        {
          "flag": "args",
          "default": [],
          "suppress_help": true,
          "nargs": "?",
          "help": ""
        }
      ],
      "comm": {
        "type": "rest",
        "method": "post",
        "target": "/{version}/maintainence/replace_node",
        "version": "v1"
      },
      "output": {
        "success": "Node Replacement Procedure Started."
      }
    }
  ]
}